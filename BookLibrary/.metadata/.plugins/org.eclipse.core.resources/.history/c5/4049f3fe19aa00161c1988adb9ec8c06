package com.llx.demo1;

import java.io.File;
import java.io.IOException;

public class Text1 {
	public static void main(String[] args) {
		File file = new File("work1.txt");
		if(file.isFile() && !file.exists()){
			try {
				file.createNewFile();
			} catch (IOException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
		}
	}

}
